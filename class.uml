@startuml toyKits
interface HumanoidToyKitFactory{
    public 
}

class RobotToyFactory implements HumanoidToyKitFactory{
    + RetroRobot createRetroRobot()
    + DogFaceRobot createDogFaceRobot()
}

interface ToyKit{
    + String getHead()
    + String getBody()
    + String getLeftHand()
    + String getRightHand()
    + String getLeftFoot()
    + String getRightFood()
    + String getSound()
    + String move()
    + String getToyName()
}

interface Product{
    + getCosts()
}

class FairyWorld{
    + createAHumanoidToyByKit(Person person, HumanoidToyKitFactory factory)
}


@enduml

